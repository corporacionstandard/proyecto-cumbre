{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets '@SavingBoxBundle/Resources/public/css/bootstrap.min.css'
    %}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
    {% endstylesheets %}
{% endblock %}
    
{% block javascripts %}
    {% javascripts '@SavingBoxBundle/Resources/public/js/jquery.min.js' 
                   '@SavingBoxBundle/Resources/public/js/bootstrap.min.js'
    %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}

{% block body %}
    
{% block navbar %}{% endblock %}

{% block anuncio %}{% endblock %}

<article {{ block('id') }}>
    {% block article %}{% endblock %}
</article>

<aside>
{% block aside %}{% endblock %}
</aside>
    
{% endblock %}